;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://Game/Player/Player.gd"
} ]
_global_script_class_icons={
"Player": ""
}

[application]

config/name="GGJ2021"
run/main_scene="res://MainScreens/MainMenu.tscn"
